{
    "name": "webparking/laravel-cash",
    "description": "Laravel API helper for CASH",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "artisaninweb/laravel-soap": "0.3.0.10",
        "myclabs/php-enum": "^1.7",
        "ext-simplexml": "*",
        "ext-dom": "*",
        "ext-soap": "*",
        "illuminate/support": "^6|^8|^9",
        "illuminate/console": "^6|^8|^9",
        "symfony/finder": "^6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpstan/phpstan": "^1",
        "friendsofphp/php-cs-fixer": "^3",
        "mockery/mockery": "^1.4",
        "illuminate/testing": "^6|^8|^9"
    },
    "autoload": {
        "psr-4": {
            "Webparking\\LaravelCash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webparking\\LaravelCash\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webparking\\LaravelCash\\Providers\\LaravelCashServiceProvider"
            ]
        }
    },
    "scripts": {
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php",
        "phpstan": "./vendor/bin/phpstan analyse src config --memory-limit=-1 --level 5",
        "phpunit": "./vendor/bin/phpunit",
        "test": [
            "composer php-cs-fixer",
            "composer phpstan",
            "composer phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
